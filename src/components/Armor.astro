---
import "../styles/style.css";
const {header, head, torso, legs, feet} = Astro.props;
---

<h2>Armor</h2>
<article class="input-article">
	<label for="head">{head}
		<input type="text" id="head">
	</label>
	<label for="torso">{torso}
		<input type="text" id="torso">
	</label>
    <label for="legs">{legs}
		<input type="text" id="legs">
	</label>
    <label for="feet">{feet}
		<input type="text" id="feet">
	</label>
</article>